generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Place {
  id           String        @id @default(cuid())
  name         String
  lat          Float
  lng          Float
  rank         Int
  class        PlaceClass
  subclass     PlaceSubClass
  keyword      String?
  province     Province
  requireCheck Boolean       @default(false)
  checkWith    String? // id of the source to check with
  createdAt    DateTime      @default(now())
  updatedAt    DateTime      @updatedAt
}

enum PlaceClass {
  amenity
}

enum PlaceSubClass {
  shop
  restaurant
  cafe
  bar
}

enum Province {
  angiang
  baclieu
  bentre
  camau
  cantho
  dongthap
  hauchiang
  kiengiang
  longan
  soctrang
  tiengiang
  travinh
  vinhlong
}
